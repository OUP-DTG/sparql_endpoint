        <rdfs:subPropertyOf rdf:resource="&owl;versionInfo"/>
    </owl:AnnotationProperty>
    


    <!-- http://languagehub.oup.com/UpperOntology/editedBy -->

    <owl:AnnotationProperty rdf:about="http://languagehub.oup.com/UpperOntology/editedBy">
        <rdfs:subPropertyOf rdf:resource="&owl;versionInfo"/>
    </owl:AnnotationProperty>
    


    <!-- http://languagehub.oup.com/UpperOntology/lastEditDate -->

    <owl:AnnotationProperty rdf:about="http://languagehub.oup.com/UpperOntology/lastEditDate">
        <rdfs:subPropertyOf rdf:resource="&owl;versionInfo"/>
    </owl:AnnotationProperty>
    


    <!-- http://languagehub.oup.com/UpperOntology/versionNumber -->

    <owl:AnnotationProperty rdf:about="http://languagehub.oup.com/UpperOntology/versionNumber">
