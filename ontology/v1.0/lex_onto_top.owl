    </owl:AnnotationProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://languagehub.oup.com/UpperOntology/abbreviationFor -->

    <owl:ObjectProperty rdf:about="http://languagehub.oup.com/UpperOntology/abbreviationFor">
        <rdfs:label xml:lang="en">abbreviation For</rdfs:label>
        <rdfs:seeAlso>http://www.lexinfo.net/ontology/2.0/lexinfo#abbreviationFor</rdfs:seeAlso>
        <owl:inverseOf rdf:resource="http://languagehub.oup.com/UpperOntology/hasAbbreviation"/>
        <rdfs:subPropertyOf rdf:resource="http://languagehub.oup.com/UpperOntology/isSpecificationOf"/>
    </owl:ObjectProperty>
    


    <!-- http://languagehub.oup.com/UpperOntology/closeMatch -->

    <owl:ObjectProperty rdf:about="http://languagehub.oup.com/UpperOntology/closeMatch">
        <rdfs:label xml:lang="en">close Match</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="http://languagehub.oup.com/UpperOntology/hasNonClassicalRelation"/>
    </owl:ObjectProperty>
    


    <!-- http://languagehub.oup.com/UpperOntology/comesAfter -->

    <owl:ObjectProperty rdf:about="http://languagehub.oup.com/UpperOntology/comesAfter">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">comes After</rdfs:label>
        <owl:inverseOf rdf:resource="http://languagehub.oup.com/UpperOntology/comesBefore"/>
        <rdfs:subPropertyOf rdf:resource="http://languagehub.oup.com/UpperOntology/hasOrdering"/>
    </owl:ObjectProperty>
    


    <!-- http://languagehub.oup.com/UpperOntology/comesBefore -->

    <owl:ObjectProperty rdf:about="http://languagehub.oup.com/UpperOntology/comesBefore">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">comes Before</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="http://languagehub.oup.com/UpperOntology/hasOrdering"/>
    </owl:ObjectProperty>
    


    <!-- http://languagehub.oup.com/UpperOntology/derivesFrom -->

    <owl:ObjectProperty rdf:about="http://languagehub.oup.com/UpperOntology/derivesFrom">
        <rdfs:label xml:lang="en">derives From</rdfs:label>
        <rdfs:seeAlso>http://purl.org/pav/derivedFrom</rdfs:seeAlso>
        <rdfs:comment xml:lang="en">The etymology can be derived from another etymology.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://languagehub.oup.com/UpperOntology/isEtymologicalRelationOf"/>
    </owl:ObjectProperty>
    


    <!-- http://languagehub.oup.com/UpperOntology/editorialNote -->

    <owl:ObjectProperty rdf:about="http://languagehub.oup.com/UpperOntology/editorialNote">
        <rdfs:label xml:lang="en">editorial Note</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="http://languagehub.oup.com/UpperOntology/hasMarker"/>
    </owl:ObjectProperty>
    


    <!-- http://languagehub.oup.com/UpperOntology/hasAbbreviation -->

    <owl:ObjectProperty rdf:about="http://languagehub.oup.com/UpperOntology/hasAbbreviation">
        <rdfs:label xml:lang="en">has Abbreviation</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="http://languagehub.oup.com/UpperOntology/hasSpecification"/>
    </owl:ObjectProperty>
    


    <!-- http://languagehub.oup.com/UpperOntology/hasAntonym -->

    <owl:ObjectProperty rdf:about="http://languagehub.oup.com/UpperOntology/hasAntonym">
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdfs:label xml:lang="en">has Antonym</rdfs:label>
        <rdfs:seeAlso>http://www.lexinfo.net/ontology/2.0/lexinfo#antonym</rdfs:seeAlso>
        <rdfs:seeAlso>http://purl.org/linguistics/gold/antonym</rdfs:seeAlso>
        <rdfs:comment xml:lang="en">refers to words that are related by having the opposite meanings to each other</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://languagehub.oup.com/UpperOntology/hasClassicalRelation"/>
    </owl:ObjectProperty>
    


    <!-- http://languagehub.oup.com/UpperOntology/hasAssociation -->

    <owl:ObjectProperty rdf:about="http://languagehub.oup.com/UpperOntology/hasAssociation">
        <rdf:type rdf:resource="&owl;IrreflexiveProperty"/>
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdfs:label xml:lang="en">has Association</rdfs:label>
        <rdfs:comment xml:lang="en">e.g nail-hammer</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://languagehub.oup.com/UpperOntology/hasNonClassicalRelation"/>
    </owl:ObjectProperty>
    


    <!-- http://languagehub.oup.com/UpperOntology/hasCanonicalForm -->

    <owl:ObjectProperty rdf:about="http://languagehub.oup.com/UpperOntology/hasCanonicalForm">
        <rdfs:label xml:lang="en">has Canonical Form</rdfs:label>
        <rdfs:seeAlso>http://lemon-model.net/lemon#canonicalForm</rdfs:seeAlso>
        <rdfs:comment xml:lang="en">The canonical (&quot;dictionary&quot;) form of the lexical entry. This can be used to indicate the &quot;lemma&quot; form of a lexical entry. (lemon)</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://languagehub.oup.com/UpperOntology/hasLexicalForm"/>
        <owl:inverseOf rdf:resource="http://languagehub.oup.com/UpperOntology/isCanonicalFormOf"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://languagehub.oup.com/UpperOntology/hasCanonicalForm"/>
            <rdf:Description rdf:about="http://languagehub.oup.com/UpperOntology/hasTransliteration"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- http://languagehub.oup.com/UpperOntology/hasCapitalization -->

    <owl:ObjectProperty rdf:about="http://languagehub.oup.com/UpperOntology/hasCapitalization">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">has Capitalisation</rdfs:label>
        <rdfs:label xml:lang="en">has Capitalization</rdfs:label>
        <rdfs:range rdf:resource="http://languagehub.oup.com/UpperOntology/Capitalization"/>
        <rdfs:subPropertyOf rdf:resource="http://languagehub.oup.com/UpperOntology/hasOrthographicStructure"/>
    </owl:ObjectProperty>
    


    <!-- http://languagehub.oup.com/UpperOntology/hasCapitonym -->

    <owl:ObjectProperty rdf:about="http://languagehub.oup.com/UpperOntology/hasCapitonym">
        <rdf:type rdf:resource="&owl;IrreflexiveProperty"/>
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdfs:label xml:lang="en">has Capitonym</rdfs:label>
        <rdfs:comment xml:lang="en">Capitonyms are words that share the same spelling but have different meanings when capitalized (and may or may not have different pronunciations). </rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://languagehub.oup.com/UpperOntology/hasHomonym"/>
    </owl:ObjectProperty>
    


    <!-- http://languagehub.oup.com/UpperOntology/hasClassicalRelation -->

    <owl:ObjectProperty rdf:about="http://languagehub.oup.com/UpperOntology/hasClassicalRelation">
        <rdfs:label xml:lang="en">has Classical Relation</rdfs:label>
        <rdfs:comment xml:lang="en">This is the superclass of common lexical relations such as synonym, antonym, etc. NOTE: this needs work. Such relations really pertain to meaning and not form units. (GOLD 2010)</rdfs:comment>
        <rdfs:seeAlso>http://purl.org/linguistics/gold/lexicalRelation</rdfs:seeAlso>
    </owl:ObjectProperty>
    


    <!-- http://languagehub.oup.com/UpperOntology/hasCloseSynonym -->

    <owl:ObjectProperty rdf:about="http://languagehub.oup.com/UpperOntology/hasCloseSynonym">
        <rdf:type rdf:resource="&owl;IrreflexiveProperty"/>
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdfs:label xml:lang="en">has Close Synonym</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="http://languagehub.oup.com/UpperOntology/hasClassicalRelation"/>
    </owl:ObjectProperty>
    


    <!-- http://languagehub.oup.com/UpperOntology/hasCollocate -->

    <owl:ObjectProperty rdf:about="http://languagehub.oup.com/UpperOntology/hasCollocate">
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdfs:label xml:lang="en">has Collocate</rdfs:label>
        <rdfs:comment xml:lang="en">words or terms that co-occur</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://languagehub.oup.com/UpperOntology/hasCompatibility -->

    <owl:ObjectProperty rdf:about="http://languagehub.oup.com/UpperOntology/hasCompatibility">
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdfs:label xml:lang="en">has Compatibility</rdfs:label>
        <rdfs:comment xml:lang="en">corresponds to the overlap between classes</rdfs:comment>
        <rdfs:domain rdf:resource="http://languagehub.oup.com/UpperOntology/SenseID"/>
        <rdfs:subPropertyOf rdf:resource="http://languagehub.oup.com/UpperOntology/hasClassicalRelation"/>
        <owl:propertyDisjointWith rdf:resource="http://languagehub.oup.com/UpperOntology/hasIncompatibility"/>
    </owl:ObjectProperty>
    


    <!-- http://languagehub.oup.com/UpperOntology/hasConsonant -->

    <owl:ObjectProperty rdf:about="http://languagehub.oup.com/UpperOntology/hasConsonant">
        <rdfs:label xml:lang="en">has Consonant</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="http://languagehub.oup.com/UpperOntology/hasSpellingStructure"/>
        <owl:propertyDisjointWith rdf:resource="http://languagehub.oup.com/UpperOntology/hasVowel"/>
    </owl:ObjectProperty>
    


    <!-- http://languagehub.oup.com/UpperOntology/hasDefinition -->

    <owl:ObjectProperty rdf:about="http://languagehub.oup.com/UpperOntology/hasDefinition">
        <rdfs:label xml:lang="en">has Definition</rdfs:label>
        <rdfs:seeAlso>http://www.w3.org/2004/02/skos/core#definition</rdfs:seeAlso>
        <rdfs:seeAlso>http://dbpedia.org/ontology/definition</rdfs:seeAlso>
        <rdfs:seeAlso>http://lemon-model.net/lemon#definition</rdfs:seeAlso>
        <rdfs:range rdf:resource="http://languagehub.oup.com/UpperOntology/Definition"/>
        <rdfs:domain rdf:resource="http://languagehub.oup.com/UpperOntology/SenseID"/>
        <rdfs:subPropertyOf rdf:resource="http://languagehub.oup.com/UpperOntology/hasSpecification"/>
    </owl:ObjectProperty>
    


    <!-- http://languagehub.oup.com/UpperOntology/hasDialect -->

    <owl:ObjectProperty rdf:about="http://languagehub.oup.com/UpperOntology/hasDialect">
        <rdfs:label xml:lang="en">has Dialect</rdfs:label>
        <rdfs:range rdf:resource="http://languagehub.oup.com/UpperOntology/Dialect"/>
        <rdfs:subPropertyOf rdf:resource="http://languagehub.oup.com/UpperOntology/hasSpecification"/>
    </owl:ObjectProperty>
    


    <!-- http://languagehub.oup.com/UpperOntology/hasDirectTranslation -->

    <owl:ObjectProperty rdf:about="http://languagehub.oup.com/UpperOntology/hasDirectTranslation">
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdfs:label xml:lang="en">has Direct Translation</rdfs:label>
        <rdfs:comment xml:lang="en">Literal translation, direct translation, or word-for-word translation is the rendering of text from one language to another one word at a time (Latin: &quot;verbum pro verbo&quot;) with or without conveying the sense of the original whole.</rdfs:comment>
        <rdfs:domain rdf:resource="http://languagehub.oup.com/UpperOntology/SenseID"/>
        <rdfs:range rdf:resource="http://languagehub.oup.com/UpperOntology/SenseID"/>
        <rdfs:subPropertyOf rdf:resource="http://languagehub.oup.com/UpperOntology/hasTranslation"/>
    </owl:ObjectProperty>
    


    <!-- http://languagehub.oup.com/UpperOntology/hasDomain -->

    <owl:ObjectProperty rdf:about="http://languagehub.oup.com/UpperOntology/hasDomain">
        <rdfs:label xml:lang="en">has Domain</rdfs:label>
        <rdfs:seeAlso>http://dbpedia.org/ontology/domain</rdfs:seeAlso>
        <rdfs:seeAlso>http://lemon-model.net/lemon#subjOfProp</rdfs:seeAlso>
        <rdfs:comment xml:lang="en">Indicates the semantic argument with represents the subjects (domain) of the property referred to by this sense. (lemon)</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://languagehub.oup.com/UpperOntology/hasLexicology"/>
    </owl:ObjectProperty>
    


    <!-- http://languagehub.oup.com/UpperOntology/hasEditorD -->

    <owl:ObjectProperty rdf:about="http://languagehub.oup.com/UpperOntology/hasEditorD">
        <rdfs:label xml:lang="en">has Editor ID</rdfs:label>
        <rdfs:range rdf:resource="http://languagehub.oup.com/UpperOntology/AgentID"/>
        <rdfs:subPropertyOf rdf:resource="http://languagehub.oup.com/UpperOntology/hasUserInformation"/>
    </owl:ObjectProperty>
    


    <!-- http://languagehub.oup.com/UpperOntology/hasEntry -->

    <owl:ObjectProperty rdf:about="http://languagehub.oup.com/UpperOntology/hasEntry">
        <rdfs:label xml:lang="en">has Entry</rdfs:label>
        <rdfs:range rdf:resource="http://languagehub.oup.com/UpperOntology/EntryID"/>
        <rdfs:domain rdf:resource="http://languagehub.oup.com/UpperOntology/LexicalEntry"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://languagehub.oup.com/UpperOntology/hasEntry"/>
            <rdf:Description rdf:about="http://languagehub.oup.com/UpperOntology/hasInflection"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- http://languagehub.oup.com/UpperOntology/hasExample -->

    <owl:ObjectProperty rdf:about="http://languagehub.oup.com/UpperOntology/hasExample">
        <rdfs:label xml:lang="en">has Example</rdfs:label>
        <rdfs:seeAlso>http://lemon-model.net/lemon#example</rdfs:seeAlso>
        <rdfs:range rdf:resource="http://languagehub.oup.com/UpperOntology/Example"/>
        <rdfs:domain rdf:resource="http://languagehub.oup.com/UpperOntology/SenseID"/>
        <rdfs:subPropertyOf rdf:resource="http://languagehub.oup.com/UpperOntology/hasSpecification"/>
    </owl:ObjectProperty>
    


    <!-- http://languagehub.oup.com/UpperOntology/hasHappensBefore -->

    <owl:ObjectProperty rdf:about="http://languagehub.oup.com/UpperOntology/hasHappensBefore">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:label xml:lang="en">has Happens Before</rdfs:label>
        <rdfs:comment xml:lang="en">e.g marry-divorce</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://languagehub.oup.com/UpperOntology/hasNonClassicalRelation"/>
    </owl:ObjectProperty>
    


    <!-- http://languagehub.oup.com/UpperOntology/hasHeterograph -->

    <owl:ObjectProperty rdf:about="http://languagehub.oup.com/UpperOntology/hasHeterograph">
        <rdf:type rdf:resource="&owl;IrreflexiveProperty"/>
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdfs:label xml:lang="en">has Heterograph</rdfs:label>
        <rdfs:comment xml:lang="en">Homophones (literally &quot;same sound&quot;) are usually defined as words that share the same pronunciation, regardless of how they are spelled. If they are spelled the same then they are also homographs (and homonyms); if they are spelled differently then they are also heterographs (literally &quot;different writing&quot;). Homographic examples include rose (flower) and rose (past tense of rise). Heterographic examples include to, too, two, and there, their, they’re. Due to their similar yet non-identical pronunciation, &quot;Ladder&quot; and &quot;Latter&quot; do not qualify as homophones, but rather synophones</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://languagehub.oup.com/UpperOntology/hasHomophone"/>
    </owl:ObjectProperty>
    


    <!-- http://languagehub.oup.com/UpperOntology/hasHeteronym -->

    <owl:ObjectProperty rdf:about="http://languagehub.oup.com/UpperOntology/hasHeteronym">
        <rdf:type rdf:resource="&owl;IrreflexiveProperty"/>
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdfs:label xml:lang="en">has Heteronym</rdfs:label>
        <rdfs:comment xml:lang="en">Heteronyms (literally &quot;different name&quot;) are the subset of homographs (words that share the same spelling) that have different pronunciations (and meanings).</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://languagehub.oup.com/UpperOntology/hasHomograph"/>
    </owl:ObjectProperty>
    


    <!-- http://languagehub.oup.com/UpperOntology/hasHolonym -->

    <owl:ObjectProperty rdf:about="http://languagehub.oup.com/UpperOntology/hasHolonym">
        <rdfs:label xml:lang="en">has Holonym</rdfs:label>
        <rdfs:seeAlso>http://www.lexinfo.net/ontology/2.0/lexinfo#holonymTerm</rdfs:seeAlso>
        <rdfs:comment xml:lang="en">Indicates that the referenced element is a part of this object. (LexInfo)</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://languagehub.oup.com/UpperOntology/hasClassicalRelation"/>
    </owl:ObjectProperty>
    


    <!-- http://languagehub.oup.com/UpperOntology/hasHomograph -->

    <owl:ObjectProperty rdf:about="http://languagehub.oup.com/UpperOntology/hasHomograph">
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdfs:label xml:lang="en">has Homograph</rdfs:label>
        <rdfs:seeAlso>http://www.lexinfo.net/ontology/2.0/lexinfo#homograph</rdfs:seeAlso>
        <rdfs:comment xml:lang="en">Homographs (literally &quot;same writing&quot;) are usually defined as words that share the same spelling, regardless of how they are pronounced.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://languagehub.oup.com/UpperOntology/hasHomonym"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://languagehub.oup.com/UpperOntology/hasCanonicalForm"/>
            <rdf:Description rdf:about="http://languagehub.oup.com/UpperOntology/isCanonicalFormOf"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- http://languagehub.oup.com/UpperOntology/hasHomonym -->

    <owl:ObjectProperty rdf:about="http://languagehub.oup.com/UpperOntology/hasHomonym">
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdfs:label xml:lang="en">has Homonym</rdfs:label>
        <rdfs:seeAlso>http://www.lexinfo.net/ontology/2.0/lexinfo#homonym</rdfs:seeAlso>
        <rdfs:comment xml:lang="en">refers to the relationship between words that are spelled or pronounced the same way but hold different meanings.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://languagehub.oup.com/UpperOntology/hasClassicalRelation"/>
    </owl:ObjectProperty>
    


    <!-- http://languagehub.oup.com/UpperOntology/hasHomophone -->

    <owl:ObjectProperty rdf:about="http://languagehub.oup.com/UpperOntology/hasHomophone">
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdfs:label>has Homophone</rdfs:label>
        <rdfs:seeAlso>http://www.lexinfo.net/ontology/2.0/lexinfo#homophone</rdfs:seeAlso>
        <rdfs:comment xml:lang="en">Homophones (literally &quot;same sound&quot;) are usually defined as words that share the same pronunciation, regardless of how they are spelled. If they are spelled the same then they are also homographs (and homonyms).</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://languagehub.oup.com/UpperOntology/hasHomonym"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://languagehub.oup.com/UpperOntology/hasPronunciation"/>
            <rdf:Description rdf:about="http://languagehub.oup.com/UpperOntology/isPronunciationOf"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- http://languagehub.oup.com/UpperOntology/hasHypernym -->

    <owl:ObjectProperty rdf:about="http://languagehub.oup.com/UpperOntology/hasHypernym">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:label xml:lang="en">has Hypernym</rdfs:label>
        <rdfs:comment xml:lang="en">In simpler terms, a hyponym shares a type-of relationship with its hypernym</rdfs:comment>
        <rdfs:seeAlso>http://www.lexinfo.net/ontology/2.0/lexinfo#hypernym</rdfs:seeAlso>
        <rdfs:subPropertyOf rdf:resource="http://languagehub.oup.com/UpperOntology/hasClassicalRelation"/>
        <owl:inverseOf rdf:resource="http://languagehub.oup.com/UpperOntology/hasHyponym"/>
    </owl:ObjectProperty>
    


    <!-- http://languagehub.oup.com/UpperOntology/hasHyphenation -->

    <owl:ObjectProperty rdf:about="http://languagehub.oup.com/UpperOntology/hasHyphenation">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">has Hyphenation</rdfs:label>
        <rdfs:range rdf:resource="http://languagehub.oup.com/UpperOntology/Hyphenation"/>
        <rdfs:subPropertyOf rdf:resource="http://languagehub.oup.com/UpperOntology/hasOrthographicStructure"/>
    </owl:ObjectProperty>
    


    <!-- http://languagehub.oup.com/UpperOntology/hasHyponym -->

    <owl:ObjectProperty rdf:about="http://languagehub.oup.com/UpperOntology/hasHyponym">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:label xml:lang="en">has Hyponym</rdfs:label>
        <rdfs:comment xml:lang="en">a word or phrase whose semantic field is more specific than its hypernym</rdfs:comment>
