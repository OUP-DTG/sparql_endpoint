    <!-- http://languagehub.oup.com/UpperOntology/hasOrthographicSpellingFrequency -->

    <owl:DatatypeProperty rdf:about="http://languagehub.oup.com/UpperOntology/hasOrthographicSpellingFrequency">
        <rdfs:label xml:lang="en">has Orthographic Spelling Frequency</rdfs:label>
