     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
    <owl:Ontology rdf:about="http://languagehub.oup.com/UpperOntology/">
        <editedBy>Artemis Parvizi</editedBy>
        <editedBy>Matt Kohl</editedBy>
        <creationDate>1 April 2015</creationDate>
        <editDate>16 November 2015</editDate>
        <createdBy>Artemis Parvizi</createdBy>
        <copyRight>Oxford University Press</copyRight>
        <owl:imports rdf:resource="http://languagehub.oup.com/"/>
        <owl:imports rdf:resource="http://languagehub.oup.com/OntoMorph"/>
        <owl:imports rdf:resource="http://languagehub.oup.com/OntoPron"/>
        <owl:imports rdf:resource="http://languagehub.oup.com/OntoEtym/"/>
        <owl:versionIRI rdf:resource="http://languagehub.oup.com/UpperOntology/0.2"/>
    </owl:Ontology>
    


