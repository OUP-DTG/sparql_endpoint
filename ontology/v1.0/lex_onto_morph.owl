        <rdfs:label xml:lang="en">Free Morpheme</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://languagehub.oup.com/UpperOntology/Morpheme"/>
        <rdfs:comment xml:lang="en">a morpheme that can stand alone as a word without another morpheme.  It does not need anything attached to it to make a word. Cat is a free morpheme.</rdfs:comment>
        <rdfs:seeAlso>http://purl.org/linguistics/gold/FreeMorpheme</rdfs:seeAlso>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/Future -->

    <Class rdf:about="&OntoMorph;Future">
