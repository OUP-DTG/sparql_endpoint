    <!-- http://languagehub.oup.com/OntoMorph/Classifier -->

    <Class rdf:about="&OntoMorph;Classifier">
        <rdfs:label xml:lang="en">Classifier</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;MarkerType"/>
        <rdfs:comment xml:lang="en">For markers (generally affixes) used to accompany nouns and to classify them depending on the type of their referent. Especially present in East Asian languages, such as Chinese and Japanese. This feature can be complemented with information on &quot;classifier type&quot;.</rdfs:comment>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/ClassifierType -->

    <Class rdf:about="&OntoMorph;ClassifierType">
        <rdfs:label xml:lang="en">Classifier</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://languagehub.oup.com/UpperOntology/GrammaticalFeature"/>
        <rdfs:comment xml:lang="en">Classifiers are affixes (prefixes, etc.) or other type of particles that accompany a noun. They are considered to classify the noun based on the type of entity the latter denotes.</rdfs:comment>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/Clause -->

