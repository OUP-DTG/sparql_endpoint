    <!-- http://languagehub.oup.com/OntoMorph/EntityType -->

    <Class rdf:about="&OntoMorph;EntityType">
        <rdfs:label xml:lang="en">Entity</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://languagehub.oup.com/UpperOntology/GrammaticalFeature"/>
        <rdfs:comment xml:lang="en">Semantic classification of nouns based on the type of reference they denote. This classification is typically applied on proper nouns for automatic information extraction purposes.</rdfs:comment>
    </Class>
    


