    <!-- http://languagehub.oup.com/OntoMorph/Person -->

    <Class rdf:about="&OntoMorph;Person">
        <rdfs:label xml:lang="en">Person</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;EntityType"/>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/PersonType -->

    <Class rdf:about="&OntoMorph;PersonType">
        <rdfs:label xml:lang="en">Person</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://languagehub.oup.com/UpperOntology/GrammaticalFeature"/>
        <rdfs:seeAlso>http://www.lexinfo.net/ontology/2.0/lexinfo#Person</rdfs:seeAlso>
        <rdfs:comment xml:lang="en">the grammatical distinction between deictic references to participant(s) in an event; typically the distinction is between the speaker, the addressee, and others. Grammatical person typically defines a language&apos;s set of personal pronouns. It also frequently affects verbs, sometimes nouns, and possessive relationships.</rdfs:comment>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/Personal -->

    <Class rdf:about="&OntoMorph;Personal">
        <rdfs:label xml:lang="en">Personal</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;ReferentialType"/>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/Phrasal -->

    <Class rdf:about="&OntoMorph;Phrasal">
        <rdfs:label xml:lang="en">Phrasal</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;UnitStructureType"/>
        <rdfs:comment xml:lang="en">Used for English phrasal verbs and any equivalent construction.</rdfs:comment>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/Phrase -->

    <Class rdf:about="&OntoMorph;Phrase">
        <rdfs:label xml:lang="en">Phrase</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;UnitStructureType"/>
        <rdfs:comment xml:lang="en">Used for units structurally more complex than a token or a compound but less than a clause, and which correspond to phrasal structures for main POS units, e.g., noun phrase, adjective phrase, etc.</rdfs:comment>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/Place -->

    <Class rdf:about="&OntoMorph;Place">
        <rdfs:label xml:lang="en">Place</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;EntityType"/>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/Plural -->

    <Class rdf:about="&OntoMorph;Plural">
        <rdfs:label xml:lang="en">Plural</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;NumberType"/>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/Polarity -->

    <Class rdf:about="&OntoMorph;Polarity">
        <rdfs:label xml:lang="en">Polarity</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;MarkerType"/>
        <rdfs:comment xml:lang="en">For markers of polarity. This feature can be complemented with information from the polarity classification.</rdfs:comment>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/PolarityType -->

    <Class rdf:about="&OntoMorph;PolarityType">
        <rdfs:label xml:lang="en">Polarity</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://languagehub.oup.com/UpperOntology/GrammaticalFeature"/>
        <rdfs:comment xml:lang="en">Expressing the two-fold distinction positive vs. negative.</rdfs:comment>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/Polite -->

    <Class rdf:about="&OntoMorph;Polite">
        <rdfs:label xml:lang="en">Polite</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;PersonType"/>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/Positive -->

    <Class rdf:about="&OntoMorph;Positive">
        <rdfs:label xml:lang="en">Positive</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;DegreeType"/>
        <rdfs:subClassOf rdf:resource="&OntoMorph;PolarityType"/>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/Possessive -->

    <Class rdf:about="&OntoMorph;Possessive">
        <rdfs:label xml:lang="en">Possessive</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;ReferentialType"/>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/Predicative -->

    <Class rdf:about="&OntoMorph;Predicative">
        <rdfs:label xml:lang="en">Predicative</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;AdjectiveFunctionType"/>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/PrepositionArticle -->

    <Class rdf:about="&OntoMorph;PrepositionArticle">
        <rdfs:label xml:lang="en">Preposition Article</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;ContractionType"/>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/Present -->

    <Class rdf:about="&OntoMorph;Present">
        <rdfs:label xml:lang="en">Present</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;TenseType"/>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/PresentGerund -->

    <Class rdf:about="&OntoMorph;PresentGerund">
        <rdfs:label xml:lang="en">Present Gerund</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;NonFinitenessType"/>
        <rdfs:comment xml:lang="en">Distinction applicable to e.g. Russian, for gerunds used to indicate an action that takes place simultaneously with the action of the main clause. For instance, equivalent to English: &apos;They sit twiddling their thumbs&apos;</rdfs:comment>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/PresentParticiple -->

    <Class rdf:about="&OntoMorph;PresentParticiple">
        <rdfs:label xml:lang="en">Present Participle</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;NonFinitenessType"/>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/Presentative -->

    <Class rdf:about="&OntoMorph;Presentative">
        <rdfs:label xml:lang="en">Presentative</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;ThematicRoleType"/>
        <rdfs:comment xml:lang="en">Applying for example to French forms voila and voici and comparable ones in other languages.</rdfs:comment>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/Proclitic -->

    <Class rdf:about="&OntoMorph;Proclitic">
        <rdfs:label xml:lang="en">Proclitic</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;CliticType"/>
        <rdfs:comment xml:lang="en">Appearing before its host.</rdfs:comment>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/Progressive -->

    <Class rdf:about="&OntoMorph;Progressive">
        <rdfs:label xml:lang="en">Progressive</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;AspectType"/>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/Pronominal -->

    <Class rdf:about="&OntoMorph;Pronominal">
        <rdfs:label xml:lang="en">Pronominal</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;SubcategorizationType"/>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/PronominalType -->

    <Class rdf:about="&OntoMorph;PronominalType">
        <rdfs:label xml:lang="en">Pronominal</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://languagehub.oup.com/UpperOntology/GrammaticalFeature"/>
        <rdfs:comment xml:lang="en">Distinctions concerning different types of pronouns and by extension too to verbs, as in some languages verbs take different types of pronouns as part of their lemma and are subsequently classified based on that (e.g., Catalan reflexive verb adonar-se, &apos;realize&apos;).</rdfs:comment>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/PronounFunctionType -->

    <Class rdf:about="&OntoMorph;PronounFunctionType">
        <rdfs:label xml:lang="en">Pronoun Function</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://languagehub.oup.com/UpperOntology/GrammaticalFeature"/>
        <rdfs:comment xml:lang="en">Expressing whether a pronoun can perform another syntactic function (e.g., determiner, adjective).</rdfs:comment>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/PronounScopeType -->

    <Class rdf:about="&OntoMorph;PronounScopeType">
        <rdfs:label xml:lang="en">Pronoun Scope</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://languagehub.oup.com/UpperOntology/GrammaticalFeature"/>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/Proper -->

    <Class rdf:about="&OntoMorph;Proper">
        <rdfs:label xml:lang="en">Proper</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;PropernessType"/>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/PropernessType -->

    <Class rdf:about="&OntoMorph;PropernessType">
        <rdfs:label xml:lang="en">Properness</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://languagehub.oup.com/UpperOntology/GrammaticalFeature"/>
        <rdfs:comment xml:lang="en">Applied to nouns and adjectives to distinguish whether they name specific individuals (people, places, organizations, etc.) or refer to non-specific entities. Proper adjectives are commonly derived from proper nouns (e.g., Shakespearian, Tibetan).</rdfs:comment>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/Punctual -->

    <Class rdf:about="&OntoMorph;Punctual">
        <rdfs:label xml:lang="en">Punctual</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;AspectDurationType"/>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/Quantitative -->

    <Class rdf:about="&OntoMorph;Quantitative">
        <rdfs:label xml:lang="en">Quantitative</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;ReferentialType"/>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/Reciprocal -->

    <Class rdf:about="&OntoMorph;Reciprocal">
        <rdfs:label xml:lang="en">Reciprocal</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;PronominalType"/>
        <rdfs:comment xml:lang="en">Pronoun used to indicate that two or more people are carrying out an action and are both simultaneously receiving the consequences of it. For example, Spanish form se accompanying the verb saludar (English &apos;greet&apos;): se saludaron (&apos;they greeted each other&apos;).</rdfs:comment>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/ReferentialType -->

    <Class rdf:about="&OntoMorph;ReferentialType">
        <rdfs:label xml:lang="en">Referential</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://languagehub.oup.com/UpperOntology/GrammaticalFeature"/>
        <rdfs:comment xml:lang="en">Applied to typical referential POS classes (pronoun, determiner, adjective, etc.) to distinguish the manner of reference they express: demonstrative, interrogative, possessive, etc.</rdfs:comment>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/ReflexivePR -->

    <Class rdf:about="&OntoMorph;ReflexivePR">
        <rdfs:label xml:lang="en">Reflexive</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;PronominalType"/>
        <rdfs:comment xml:lang="en">Pronoun expressing that the object of a sentence is the same as the subject, who therefore both carries out the action and receives its benefits.</rdfs:comment>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/Relative -->

    <Class rdf:about="&OntoMorph;Relative">
        <rdfs:label xml:lang="en">Relative</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;ReferentialType"/>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/ResidualType -->

    <Class rdf:about="&OntoMorph;ResidualType">
        <rdfs:label xml:lang="en">Residual</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://languagehub.oup.com/UpperOntology/GrammaticalFeature"/>
        <rdfs:comment xml:lang="en">Types for residual lexical units, such as abbreviations and symbols. Note: These elements can also be analysed as belonging to different POS classes, but for the moment we leave them in an independent class.</rdfs:comment>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/Second -->

    <Class rdf:about="&OntoMorph;Second">
        <rdfs:label xml:lang="en">Second</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;PersonType"/>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/Semelfactive -->

    <Class rdf:about="&OntoMorph;Semelfactive">
        <rdfs:label xml:lang="en">Semelfactive</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;AktionsartType"/>
        <rdfs:comment xml:lang="en">Denoting an event that has no duration (see Aspect Duration Type section) and is atelic (see Aspect telicity Type section).</rdfs:comment>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/Semiauxiliary -->

    <Class rdf:about="&OntoMorph;Semiauxiliary">
        <rdfs:label>Semiauxiliary</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;VerbType"/>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/Semideponent -->

    <Class rdf:about="&OntoMorph;Semideponent">
        <rdfs:label>Semideponent</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;VoiceType"/>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/Sensory -->

    <Class rdf:about="&OntoMorph;Sensory">
        <rdfs:label xml:lang="en">Sensory</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;EvidentialityType"/>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/SentenceModality -->

    <Class rdf:about="&OntoMorph;SentenceModality">
        <rdfs:label xml:lang="en">Sentence Modality</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;MarkerType"/>
        <rdfs:comment xml:lang="en">For markers of sentence modality (exclamative, imperative, etc.). This feature can be complemented with information from the semtence modality classification.</rdfs:comment>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/SentenceModalityType -->

    <Class rdf:about="&OntoMorph;SentenceModalityType">
        <rdfs:label xml:lang="en">Sentence Modality</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://languagehub.oup.com/UpperOntology/GrammaticalFeature"/>
        <rdfs:comment xml:lang="en">Distinctions relative to type of sentence. Needed for fully typifying particles in charge of expressing this type of information in certain languages.</rdfs:comment>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/SetPhrase -->

    <Class rdf:about="&OntoMorph;SetPhrase">
        <rdfs:label xml:lang="en">Set Phrase</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;IdiomaticType"/>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/Singular -->

    <Class rdf:about="&OntoMorph;Singular">
        <rdfs:label xml:lang="en">Singular</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;NumberType"/>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/State -->

    <Class rdf:about="&OntoMorph;State">
        <rdfs:label xml:lang="en">State</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;AktionsartType"/>
        <rdfs:comment xml:lang="en">Denoting situations that have duration (see Aspect Duration Type section) and changeless.</rdfs:comment>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/StemVowelType -->

    <Class rdf:about="&OntoMorph;StemVowelType">
        <rdfs:label xml:lang="en">Stem Vowel</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://languagehub.oup.com/UpperOntology/GrammaticalFeature"/>
        <OntoMorph:languageSpecific xml:lang="en">Arabic</OntoMorph:languageSpecific>
        <rdfs:comment xml:lang="en">Classification exclusive to the Arabic verbal system. Indicates which vowel is used in the imperfect form of the verb. All verbs of verb form Arabic I must have a stem vowel value.</rdfs:comment>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/Strong -->

    <Class rdf:about="&OntoMorph;Strong">
        <rdfs:label xml:lang="en">Strong</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;InflectionType"/>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/StructuralRelation -->

    <Class rdf:about="&OntoMorph;StructuralRelation">
        <rdfs:label xml:lang="en">Structural Relation</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;MarkerType"/>
        <rdfs:comment xml:lang="en">For markers of distinctions relative to structural relation (coordination, subordination). This information can be complemented with the feature structural relation.</rdfs:comment>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/StructuralRelationType -->

    <Class rdf:about="&OntoMorph;StructuralRelationType">
        <rdfs:label xml:lang="en">Structural Relation</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://languagehub.oup.com/UpperOntology/GrammaticalFeature"/>
        <rdfs:comment xml:lang="en">Classifying the type of relation between two clausal units: coordination or subordination. This feature is needed for markers (particles, affixes) of coordination and subordination present in some languages.</rdfs:comment>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/Subcategorization -->

    <Class rdf:about="&OntoMorph;Subcategorization">
        <rdfs:label xml:lang="en">Subcategorization</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;MarkerType"/>
        <rdfs:comment xml:lang="en">For markers of subcategorization distinctions. For example, for representing copulative prefixes in Zulu: part of speech=prefix, marker type=subcategorization, subcategorization=copulative.</rdfs:comment>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/SubcategorizationType -->

    <Class rdf:about="&OntoMorph;SubcategorizationType">
        <rdfs:label xml:lang="en">Subcategorization</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://languagehub.oup.com/UpperOntology/GrammaticalFeature"/>
        <rdfs:comment xml:lang="en">Expressing the different types of subcategorization behavior. At the moment the class names are based on the widely spread dictionary distinctions, very oriented towards verbal classification.</rdfs:comment>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/Subject -->

    <Class rdf:about="&OntoMorph;Subject">
        <rdfs:label xml:lang="en">Subject</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;ConcordType"/>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/Subjunctive -->

    <Class rdf:about="&OntoMorph;Subjunctive">
        <rdfs:label xml:lang="en">Subjunctive</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;MoodType"/>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/Subordination -->

    <Class rdf:about="&OntoMorph;Subordination">
        <rdfs:label xml:lang="en">Subordination</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;StructuralRelationType"/>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/Superlative -->

    <Class rdf:about="&OntoMorph;Superlative">
        <rdfs:label xml:lang="en">Superlative</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;DegreeType"/>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/SupineI -->

    <Class rdf:about="&OntoMorph;SupineI">
        <rdfs:label xml:lang="en">Supine I</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;NonFinitenessType"/>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/SupineII -->

    <Class rdf:about="&OntoMorph;SupineII">
        <rdfs:label xml:lang="en">Supine II</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;NonFinitenessType"/>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/Symbol -->

    <Class rdf:about="&OntoMorph;Symbol">
        <rdfs:label xml:lang="en">Symbol</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;ResidualType"/>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/Telic -->

    <Class rdf:about="&OntoMorph;Telic">
        <rdfs:label xml:lang="en">Telic</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;AspectTelicityType"/>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/Temporal -->

    <Class rdf:about="&OntoMorph;Temporal">
        <rdfs:label xml:lang="en">Temporal</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;ThematicRoleType"/>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/Tense -->

    <Class rdf:about="&OntoMorph;Tense">
        <rdfs:label xml:lang="en">Tense</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;MarkerType"/>
        <rdfs:comment xml:lang="en">For markers of tense. This information can be complemented with the feature tense.</rdfs:comment>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/TenseType -->

    <Class rdf:about="&OntoMorph;TenseType">
        <rdfs:label xml:lang="en">Tense</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://languagehub.oup.com/UpperOntology/GrammaticalFeature"/>
        <rdfs:comment xml:lang="en">Distinctions relative to tense. Aiming at covering only simple forms. Therefore, forms such as Spanish &apos;preterito pluscuamperfecto&apos; (e.g., habia cantado) are disregarded.
Distinctions involving grammatical aspect (e.g., between Spanish &apos;preterito imperfecto&apos; cantaba and &apos;preterito perfecto&apos; simple cante) are dealt with in combination with values in the Aspect Type classification</rdfs:comment>
        <rdfs:seeAlso>http://www.lexinfo.net/ontology/2.0/lexinfo#Tense</rdfs:seeAlso>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/ThematicRole -->

    <Class rdf:about="&OntoMorph;ThematicRole">
        <rdfs:label xml:lang="en">Thematic Role</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;MarkerType"/>
        <rdfs:comment xml:lang="en">For markers of thematic role. This feature can be complemented with information from the thematic role classification.</rdfs:comment>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/ThematicRoleType -->

    <Class rdf:about="&OntoMorph;ThematicRoleType">
        <rdfs:label xml:lang="en">Thematic Role</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://languagehub.oup.com/UpperOntology/GrammaticalFeature"/>
        <rdfs:comment xml:lang="en">Describing whether the unit expresses semantic information characteristic of certain adjuncts; i.e., time, place, direction, instrument, etc.

Note: The term thematic role is being used here is a slightly broader sense than in the standard linguistic literature, as it includes distinctions also hold by adjunts (e.g., presentative, associative), but that are not generally considered thematic relations.</rdfs:comment>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/Third -->

    <Class rdf:about="&OntoMorph;Third">
        <rdfs:label xml:lang="en">Third</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;PersonType"/>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/Time -->

    <Class rdf:about="&OntoMorph;Time">
        <rdfs:label xml:lang="en">Time</rdfs:label>
        <rdfs:subClassOf rdf:resource="&OntoMorph;EntityType"/>
    </Class>
    


