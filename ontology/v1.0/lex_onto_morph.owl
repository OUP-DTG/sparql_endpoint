        <rdfs:label xml:lang="en">Event Modality</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://languagehub.oup.com/UpperOntology/GrammaticalFeature"/>
        <rdfs:comment xml:lang="en">Type of modality expressed at the event level. It can be applied to different POS classes, typically modal verb in languages like English (e.g., should), but also adjectives (probable), adverbs (possibly, perhaps), etc.</rdfs:comment>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/Evidential -->

    <Class rdf:about="&OntoMorph;Evidential">
