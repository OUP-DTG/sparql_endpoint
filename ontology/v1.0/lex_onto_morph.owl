        <rdfs:comment xml:lang="en">Classifying different types of evidentiality markers.</rdfs:comment>
    </Class>
    


    <!-- http://languagehub.oup.com/OntoMorph/Exclamative -->

    <Class rdf:about="&OntoMorph;Exclamative">
